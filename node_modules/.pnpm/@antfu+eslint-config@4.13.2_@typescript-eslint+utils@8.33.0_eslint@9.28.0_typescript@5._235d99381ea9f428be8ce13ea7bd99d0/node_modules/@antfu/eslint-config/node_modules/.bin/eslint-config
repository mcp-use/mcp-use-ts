else
  export NODE_PATH="/Users/pietrozullo/mcp-use/mcp-use-ts/node_modules/.pnpm/@antfu+eslint-config@4.13.2_@typescript-eslint+utils@8.33.0_eslint@9.28.0_typescript@5._235d99381ea9f428be8ce13ea7bd99d0/node_modules/@antfu/eslint-config/bin/node_modules:/Users/pietrozullo/mcp-use/mcp-use-ts/node_modules/.pnpm/@antfu+eslint-config@4.13.2_@typescript-eslint+utils@8.33.0_eslint@9.28.0_typescript@5._235d99381ea9f428be8ce13ea7bd99d0/node_modules/@antfu/eslint-config/node_modules:/Users/pietrozullo/mcp-use/mcp-use-ts/node_modules/.pnpm/@antfu+eslint-config@4.13.2_@typescript-eslint+utils@8.33.0_eslint@9.28.0_typescript@5._235d99381ea9f428be8ce13ea7bd99d0/node_modules/@antfu/node_modules:/Users/pietrozullo/mcp-use/mcp-use-ts/node_modules/.pnpm/@antfu+eslint-config@4.13.2_@typescript-eslint+utils@8.33.0_eslint@9.28.0_typescript@5._235d99381ea9f428be8ce13ea7bd99d0/node_modules:/Users/pietrozullo/mcp-use/mcp-use-ts/node_modules/.pnpm/node_modules:$NODE_PATH"
